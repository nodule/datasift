{
  "name": "api",
  "ns": "datashift",
  "title": "DataShift api",
  "description": "Api to datashift",
  "phrases": {
    "active": "Connecting to datashift"
  },
  "require": {
    "datashift": "datashift/NodeJS-Consumer"
  },
  "ports": {
    "input": {
      "username": {
        "title": "Username",
        "type": "string",
        "required": "true"
      },
      "api_key": {
        "title": "Api Key",
        "type": "string",
        "required": "true"
      }
    },
    "output": {
      "consumer": {
        "type": "function"
      }
    }
  }
}
