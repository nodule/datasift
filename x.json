        },
        "output": {
          "consumer": {
            "type": "function"
          }
        }
      },
      "phrases": {
        "active": "Connecting to datasift"
      },
      "description": "Api to datasift",
      "title": "Datasift api",
      "ns": "datasift",
      "name": "api",
      "env": "server",
      "fn": "var consumer = new datasift($.username, $.api_key);\nconsumer.connect();\n\noutput = {\n  consumer: $.create(consumer)\n};\n"
    },
    {
      "dependencies": {
        "npm": {
          "datasift": "datasift/NodeJS-Consumer"
        }
      },
      "ports": {
        "input": {
          "username": {
            "title": "Username",
            "type": "string",
            "required": true
          },
          "api_key": {
            "title": "Api Key",
            "type": "string",
            "required": true
          },
          "connect": {
            "title": "Connect",
            "type": "function",
            "required": false
          },
          "error": {
            "title": "Error",
            "type": "function",
            "required": false
          },
          "warning": {
            "title": "Warning",
            "type": "function",
            "required": false
          },
          "disconnect": {
            "title": "Disconnect",
            "type": "function",
            "required": false
          },
          "interaction": {
            "title": "Interaction",
            "type": "function",
            "required": false
          },
          "delete": {
            "title": "Delete",
            "type": "function",
            "required": false
          }
        },
        "output": {
          "consumer": {
            "type": "function"
          }
        }
      },
      "phrases": {
        "active": "Connecting to datasift"
      },
      "description": "Connect to datasift",
      "title": "DataSift Connect",
      "ns": "datasift",
      "name": "connect",
      "env": "server",
      "fn": "var consumer = new datasift($.username, $.api_key);\nconsumer.connect();\n\n//Emitted when stream is connected\nif ($.connect) consumer.on('connect', $.connect);\n\n//Emitted when there is an error\nif ($.error) consumer.on('error', $.error);\n\n//Emitted when there is a warning\nif ($.warning) consumer.on('warning', $.warning);\n\n//Emitted when disconnected\nif ($.disconnect) consumer.on('disconnect', $.disconnect);\n\n//Emitted when an interaction is received\nif ($.interaction) consumer.on('interaction', $.interaction);\n\n//Emitted when a delete message is received\nif ($.delete) consumer.on('delete', $.delete);\n\noutput = {\n  consumer: $.create(consumer)\n};\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": true
          }
        },
        "output": {
          "data": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datasift Delete"
      },
      "description": "Delete",
      "title": "DataSift Delete",
      "ns": "datasift",
      "name": "delete",
      "env": "server",
      "fn": "output = [$.consumer, 'on', 'delete']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": true
          }
        },
        "output": {
          "out": {
            "type": "any"
          }
        }
      },
      "phrases": {
        "active": "Datasift disconnect"
      },
      "description": "disconnect",
      "title": "DataSift disconnect",
      "ns": "datasift",
      "name": "disconnect",
      "env": "server",
      "fn": "output = [$.consumer, 'on', 'disconnect']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": true
          }
        },
        "output": {
          "error": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datasift error"
      },
      "description": "error",
      "title": "DataSift error",
      "ns": "datasift",
      "name": "error",
      "env": "server",
      "fn": "output = [$.consumer, 'on', 'error']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": true
          }
        },
        "output": {
          "data": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datasift interaction"
      },
      "description": "interaction",
      "title": "DataSift interaction",
      "ns": "datasift",
      "name": "interaction",
      "env": "server",
      "fn": "output = [$.consumer, 'on', 'interaction']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": true
          },
          "hash": {
            "title": "Service Hash",
            "type": "string",
            "required": true
          }
        },
        "output": {}
      },
      "phrases": {
        "active": "Datasift subscribe"
      },
      "description": "subscribe",
      "title": "DataSift subscribe",
      "ns": "datasift",
      "name": "subscribe",
      "env": "server",
      "fn": "on.input.hash = function() {\n  $.consumer.subscribe($.hash);\n};\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": true
          },
          "hash": {
            "title": "Service Hash",
            "type": "string",
