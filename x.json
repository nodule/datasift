{
  "name": "datasift",
  "description": "Datasift for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/datasift.git"
  },
  "dependencies": {
    "datasift": "datasift/NodeJS-Consumer"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "username": {
            "title": "Username",
            "type": "string",
            "required": "true"
          },
          "api_key": {
            "title": "Api Key",
            "type": "string",
            "required": "true"
          }
        },
        "output": {
          "consumer": {
            "type": "function"
          }
        }
      },
      "require": {
        "datasift": "datasift/NodeJS-Consumer"
      },
      "phrases": {
        "active": "Connecting to datasift"
      },
      "description": "Api to datasift",
      "title": "Datasift api",
      "ns": "datasift",
      "name": "api",
      "fn": "var consumer = new datashift(input.username, input.api_key);\nconsumer.connect();\n\noutput = {\n  consumer: consumer\n};\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "data": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datasift Delete"
      },
      "description": "Delete",
      "title": "DataSift Delete",
      "ns": "datasift",
      "name": "delete",
      "fn": "output = [input.consumer, 'on', 'delete']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "out": {
            "type": "any"
          }
        }
      },
      "phrases": {
        "active": "Datasift disconnect"
      },
      "description": "disconnect",
      "title": "DataSift disconnect",
      "ns": "datasift",
      "name": "disconnect",
      "fn": "output = [input.consumer, 'on', 'disconnect']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "error": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datasift error"
      },
      "description": "error",
      "title": "DataSift error",
      "ns": "datasift",
      "name": "error",
      "fn": "output = [input.consumer, 'on', 'error']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "data": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datasift interaction"
      },
      "description": "interaction",
      "title": "DataSift interaction",
      "ns": "datasift",
      "name": "interaction",
      "fn": "output = [input.consumer, 'on', 'interaction']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          },
          "hash": {
            "title": "Service Hash",
            "type": "string",
            "required": "true"
          }
        },
        "output": {}
      },
      "phrases": {
        "active": "Datasift subscribe"
      },
      "description": "subscribe",
      "title": "DataSift subscribe",
      "ns": "datasift",
      "name": "subscribe",
      "fn": "input.consumer.subscribe(input.hash);\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          },
          "hash": {
            "title": "Service Hash",
            "type": "string",
            "required": "true"
          }
        },
        "output": {}
      },
      "phrases": {
        "active": "Datasift unsubscribe"
      },
      "description": "unsubscribe",
      "title": "DataSift unsubscribe",
      "ns": "datasift",
      "name": "unsubscribe",
      "fn": "input.consumer.unsubscribe(input.hash);\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "warning": {
            "type": "string"
          }
        }
      },
      "phrases": {
        "active": "Datasift warning"
      },
      "description": "warning",
      "title": "DataSift warning",
      "ns": "datasift",
      "name": "warning",
      "fn": "output = [input.consumer, 'on', 'warning']\n"
    }
  ],
  "twigs": []
}