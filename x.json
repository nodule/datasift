      "description": "unsubscribe",
      "title": "DataSift unsubscribe",
      "ns": "datasift",
      "name": "unsubscribe",
      "env": "server",
      "fn": "on.input.hash = function() {\n  $.consumer.unsubscribe($.hash);\n};\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
