{
  "name": "datasift",
  "description": "Datasift for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/datasift.git"
  },
  "dependencies": {
    "datasift": "datasift/NodeJS-Consumer"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "username": {
            "title": "Username",
            "type": "string",
            "required": "true"
          },
          "api_key": {
            "title": "Api Key",
            "type": "string",
            "required": "true"
          }
        },
        "output": {
          "consumer": {
            "type": "function"
          }
        }
      },
      "require": {
        "datashift": "datashift/NodeJS-Consumer"
      },
      "phrases": {
        "active": "Connecting to datashift"
      },
      "description": "Api to datashift",
      "title": "DataShift api",
      "ns": "datashift",
      "name": "api",
      "fn": "var consumer = new datashift(input.username, input.api_key);\nconsumer.connect();\n\noutput = {\n  consumer: consumer\n};\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "data": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datashift Delete"
      },
      "description": "Delete",
      "title": "DataShift Delete",
      "ns": "datashift",
      "name": "delete",
      "fn": "output = [input.consumer, 'on', 'delete']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "out": {
            "type": "any"
          }
        }
      },
      "phrases": {
        "active": "Datashift disconnect"
      },
      "description": "disconnect",
      "title": "DataShift disconnect",
      "ns": "datashift",
      "name": "disconnect",
      "fn": "output = [input.consumer, 'on', 'disconnect']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "error": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datashift error"
      },
      "description": "error",
      "title": "DataShift error",
      "ns": "datashift",
      "name": "error",
      "fn": "output = [input.consumer, 'on', 'error']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "data": {
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Datashift interaction"
      },
      "description": "interaction",
      "title": "DataShift interaction",
      "ns": "datashift",
      "name": "interaction",
      "fn": "output = [input.consumer, 'on', 'interaction']\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          },
          "hash": {
            "title": "Service Hash",
            "type": "string",
            "required": "true"
          }
        },
        "output": {}
      },
      "phrases": {
        "active": "Datashift subscribe"
      },
      "description": "subscribe",
      "title": "DataShift subscribe",
      "ns": "datashift",
      "name": "subscribe",
      "fn": "input.consumer.subscribe(input.hash);\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          },
          "hash": {
            "title": "Service Hash",
            "type": "string",
            "required": "true"
          }
        },
        "output": {}
      },
      "phrases": {
        "active": "Datashift unsubscribe"
      },
      "description": "unsubscribe",
      "title": "DataShift unsubscribe",
      "ns": "datashift",
      "name": "unsubscribe",
      "fn": "input.consumer.unsubscribe(input.hash);\n"
    },
    {
      "ports": {
        "input": {
          "consumer": {
            "title": "Consumer",
            "type": "function",
            "required": "true"
          }
        },
        "output": {
          "warning": {
            "type": "string"
          }
        }
      },
      "phrases": {
        "active": "Datashift warning"
      },
      "description": "warning",
      "title": "DataShift warning",
      "ns": "datashift",
      "name": "warning",
      "fn": "output = [input.consumer, 'on', 'warning']\n"
    }
  ],
  "twigs": []
}